modules = ["python-3.11", "postgresql-16"]
[agent]
integrations = ["python_openai:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["cargo", "ffmpeg", "freetype", "glibcLocales", "lcms2", "libiconv", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "openssl", "pkg-config", "rustc", "tcl", "tk", "zlib"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Sistema Completo"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Medical Records System"

[[workflows.workflow]]
name = "Medical Records System"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "Admin Panel"
author = 44652201
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app_admin.py --server.port 5001 --server.address 0.0.0.0"

[[workflows.workflow]]
name = "Sistema Completo"
author = 44652201
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000 --server.address 0.0.0.0"

[[ports]]
localPort = 5000
externalPort = 80

